{
    "manifest_version": 3,
    "name": "Context Menu Translate",
    "description": "Easy Translation Via Context Menu",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16":"icon.png",
        "48":"icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["contextMenus", "storage"]
  }